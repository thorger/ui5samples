<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
	<ViewSettingsDialog confirm="onGroupingDialogConfirmed" id="groupingViewSettingsDialog">
		<groupItems>
			<ViewSettingsItem id="mainCategoryGrouper" key="MainCategoryName" text="{/#Product/MainCategoryName/@sap:label}"/>
			<ViewSettingsItem id="subcategoryGrouper" key="SubCategoryName" text="{/#Product/SubCategoryName/@sap:label}"/>
			<ViewSettingsItem id="quantityGrouper" key="StockQuantity" text="{/#Product/StockQuantity/@sap:label}"/>
			<ViewSettingsItem id="priceGrouper" key="Price" text="{/#Product/Price/@sap:label}"/>
			<ViewSettingsItem id="ratingGrouper" key="AverageRating" text="{/#Product/AverageRating/@sap:label}"/>
			<ViewSettingsItem id="supplierGrouper" key="SupplierName" text="{/#Product/SupplierName/@sap:label}"/>
		</groupItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>